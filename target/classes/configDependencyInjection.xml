<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean class="com.springcore.dependencyInjection.Student" name = "StudentA">
        <property name="id">
            <value>7875</value>
        </property>

        <property name="name">
            <value>Sumit</value>
        </property>
        
        <property name="address">
            <value>Hyderabad</value>
        </property>
    </bean>

    <!--Second Bean using different tag -->
    <bean class="com.springcore.dependencyInjection.Student" name = "StudentB">
        <property name="id" value="68746"/>
        <property name="name" value="Ravi"/>
        <property name="address" value = "Delhi"/>
    </bean>
</beans>